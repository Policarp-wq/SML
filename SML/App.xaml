<Application x:Class="SML.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModel ="clr-namespace:SML.ViewModels"
             xmlns:view ="clr-namespace:SML.Views"
             xmlns:local="clr-namespace:SML"
             >
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="cross" UriSource="Assets/cross.png"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/TextBoxTheme.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
